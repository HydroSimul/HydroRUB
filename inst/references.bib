  author = {Clark, Martyn P. and Slater, Andrew G. and Rupp, David E. and Woods, Ross A. and Vrugt, Jasper A. and Gupta, Hoshin V. and Wagener, Thorsten and Hay, Lauren E.},
  year = {2008},
  journal = {Water Resources Research},
  volume = {44},
