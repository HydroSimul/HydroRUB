@techreport{MARRoT_knoben_2019,
  type = {Preprint},
  title = {Modular {{Assessment}} of {{Rainfall-Runoff Models Toolbox}} ({{MARRMoT}}) v1.0: An Open-Source, Extendable Framework Providing Implementations of 46 Conceptual Hydrologic Models as Continuous Space-State Formulations},
  shorttitle = {Modular {{Assessment}} of {{Rainfall-Runoff Models Toolbox}} ({{MARRMoT}}) v1.0},
