  author = {McManamay, Ryan A. and Kc, Binita and {Allen-Dumas}, Melissa R. and Kao, Shih-Chieh and Brelsford, Christa M. and Ruddell, Benjamin L. and Sanyal, Jibonananda and Stewart, Robert N. and Bhaduri, Budhendra L.},
  year = {2021},
  journal = {Water Resources Research},
