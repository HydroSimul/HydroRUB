@article{fujimoriDownscalingGlobalEmissions2017,
  title = {Downscaling {{Global Emissions}} and {{Its Implications Derived}} from {{Climate Model Experiments}}},
  author = {Fujimori, Shinichiro and Abe, Manabu and Kinoshita, Tsuguki and Hasegawa, Tomoko and Kawase, Hiroaki and Kushida, Kazuhide and Masui, Toshihiko and Oka, Kazutaka and Shiogama, Hideo and Takahashi, Kiyoshi and Tatebe, Hiroaki and Yoshikawa, Minoru},
