@article{EvaluatDownscaledData_behnke_2016,
  title = {Evaluation of Downscaled, Gridded Climate Data for the Conterminous {{United States}}},
  author = {Behnke, R. and Vavrus, S. and Allstadt, A. and Albright, T. and Thogmartin, W. E. and Radeloff, V. C.},
  year = {2016},
