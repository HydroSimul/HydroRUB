@article{CHELSA_karger_2023,
  title = {{{CHELSA-W5E5}}: Daily 1\&thinsp;Km Meteorological Forcing Data for Climate Impact Studies},
  shorttitle = {{{CHELSA-W5E5}}},
  author = {Karger, Dirk Nikolaus and Lange, Stefan and Hari, Chantal and Reyer, Christopher P. O. and Conrad, Olaf and Zimmermann, Niklaus E. and Frieler, Katja},
  year = {2023},
