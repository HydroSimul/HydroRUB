@article{MA_lei_2023,
  title = {{Analyse der Sensitivit{\"a}t von Entscheidungen bei Aufbau und Anwendung des Modells EDCHM und der Unsicherheit von Wasserhaushaltsberechnungen anhand s{\"a}chsischer Einzugsgebiete}},
  author = {Lei, Kan},
  year = {2023},
  abstract = {The present master's thesis examines uncertainty and sensitivity in hydrological modeling, focusing mainly on the model structure. The goal of the analysis is to identify model construction and assess uncertainty in modeling. The focus is on the independent and dependent elements from five model construction elements: parameters, boundary conditions, physiographic data, module, process, and model. The analysis is conducted both before and after model calibration, using the EDCHM model framework, and a total of 34 Saxony catchments are examined. To investigate uncertainty, output data distributions and crash-tests are conducted, while sensitivity indices, improvement indices, and parameter-class indices are analyzed for sensitivity. The analysis shows that different catchments have different sensitivities to processes, but in main catchments, the differences are significantly smaller. Model parameters are sensitive to various physiographic data. Therefore, before applying a modular model framework, it is recommended to investigate the uncertainty and sensitivity of model construction for specific catchments. The discretization of model unit surfaces and boundary condition data is beneficial for modeling in all catchments. Despite model calibration, uncertainties in the water balance persist. The study demonstrates the all-around uncertainty in hydrological modeling and the strong method of analyzing sensitivity. In the end, a strategy is recommended to identify the model structure for specific catchment.},
  langid = {ngerman},
  file = {C:\Users\lei\Zotero\storage\5ESK8FRD\Lei - Analyse der Sensitivität von Entscheidungen bei Au.pdf}
}

